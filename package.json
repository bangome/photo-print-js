{
  "name": "photo-print-js",
  "version": "0.1.2",
  "description": "JavaScript library for photo print layouts - similar to Windows Photo Printing Wizard",
  "type": "module",
  "main": "dist/photo-print.cjs.js",
  "module": "dist/photo-print.es.js",
  "browser": "dist/photo-print.umd.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/photo-print.es.js",
      "require": "./dist/photo-print.cjs.js",
      "types": "./dist/types/index.d.ts"
    },
    "./ui": {
      "import": "./dist/photo-print-ui.es.js",
      "require": "./dist/photo-print-ui.cjs.js"
    },
    "./ui.css": "./dist/ui.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "photo",
    "print",
    "layout",
    "image",
    "pdf",
    "grid",
    "collage",
    "printing",
    "wizard"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "jsdom": "^27.3.0",
    "puppeteer": "^24.34.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "jspdf": "^2.5.0"
  },
  "peerDependenciesMeta": {
    "jspdf": {
      "optional": true
    }
  }
}
